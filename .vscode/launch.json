    "version": "0.2.0",
    "configurations": [
        {
            "name": "Python: Flask Reset db",
            "type": "python",
            "request": "launch",
            "module": "flask",
            "console": "internalConsole",
            "env": {
                "FLASK_APP": "app.py",
                "FLASK_ENV": "development",
                "FLASK_DEBUG": "0"
            },
            "args": [
                
                "reset_db"
            ],
            "jinja": true
        },
        {
            "name": "Python: Flask Test db",
            "type": "python",
            "request": "launch",
            "module": "flask",
            "console": "internalConsole",
            "env": {
                "FLASK_APP": "app.py",
                "FLASK_ENV": "development",
                "FLASK_DEBUG": "0"
            },
            "args": [
                "test_db"
            ],
            "jinja": true
        }
    ]
}